<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.justi.ownapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/julius"
        android:label="Julius"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LogInActivity"
            android:label="Log in">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FoundItemsActivity"
            android:label="Found Lyrics"
            android:launchMode="singleInstance"/>
        <activity
            android:name=".WholeLyricActivity"
            android:label="Information" />
        <activity
            android:name=".RememberedActivity"
            android:label="Saved Songs" />
        <activity
            android:name=".SearchActivity"
            android:label="Find Song" />/>
        <activity
            android:name=".MainActivity"
            android:label="Register"
            >

        </activity>
    </application>

</manifest>